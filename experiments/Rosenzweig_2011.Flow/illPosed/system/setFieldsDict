FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue h -0.50
);

regions
(
    boxToCell
    {
        box (0.0 0.0 0.00) (0.08 0.08 0.40);
        fieldValues
        (
            volScalarFieldValue h -0.10
        );
    }
);


