FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// [kg m s K mol * *]

// Clogging parameters
clog?     true;

nMin       nMin       [0 0 0 0 0 0 0]    0.05;     // Minimum porosity
permMin    permMin    [0 2 0 0 0 0 0]    1.0E-14;  // Minimum permeability

nRef       nRef       [0 0 0 0 0 0 0]    0.39;     // Reference porosity
permRef    permRef    [0 2 0 0 0 0 0]    9.99E-11;  // Reference permeability

cloggingModel powerLaw;

powerLawCoeffs
{
    nExponent dimless 3;     //- 19/6 if Clement et al. (1996) 
}

kozenyCarmanCoeffs
{
    nExponent dimless 3.0;
    mExponent dimless 2.0;
}

