     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000";
    object      perm;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 0 0 0 0 0 ];
/*
    K = k rho g / mu

    Hydraulic conductivity K = 1.24 cm/min = 2.067E-4 m/s
    Water density rho = 1000 kg/m³
    Dynamic viscosity mu = 0.001 Ns/m²
    
Permeability 
    k   = K mu / rho g
        = 2.067E-4 * 0.001 / 9.81 * 1000
        = 2.107e-11 m²
*/
internalField   uniform 2.107e-11;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    right
    {
        type            empty;
    }
    left
    {
        type            empty;
    }
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
}


// ************************************************************************* //
