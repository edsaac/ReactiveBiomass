actions
(
  {       // Set an inoculum  strip
    name    inoculumStripCellSet;
    type    cellSet;
    action  new;
    source  boxToCell ;
    sourceInfo
    {
        //box     (0.000 0.000 0.140) (0.300 0.010 0.270);
        box     (0.000 0.000 0.00) (0.08 0.08 0.600);
    }
  }
  
  {
    name    inoculumStrip;
    type    cellZoneSet;
    action  new;
    source  setToCellZone;
    sourceInfo
    {
        set     inoculumStripCellSet;
    }
  }
);