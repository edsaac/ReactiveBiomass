FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
// [kg m s K mol * *]

// Densities
rho       rho       [1 -3 0 0 0 0 0]    1000.0;
rho_X     rho_X     [1 -3 0 0 0 0 0]    5.0;

//Other physical stuff
g         g         [0 1 -2 0 0 0 0]    9.81;
mu        mu        [1 -1 -1 0 0 0 0]   0.0008891;

//Dispersion
mDif       0; //2.0E-9;
mDifAir    2.06E-5; // 0.206 cmÂ²/s [Berlin 2014]
LongDisp   0; //0.01;
TransDisp  0; //0.001;

molDiff    molDiff    [0 2 -1 0 0 0 0]
    ($mDif 0.0 0.0 
     0.0 $mDif 0.0 
     0.0 0.0 $mDif);

molDiff_air    molDiff_air    [0 2 -1 0 0 0 0]
     ($mDifAir 0.0 0.0 
      0.0 $mDifAir 0.0 
      0.0 0.0 $mDifAir);

DispTensor DispTensor [0 1 0 0 0 0 0]
    ($LongDisp $TransDisp $TransDisp 
     $TransDisp $LongDisp $TransDisp 
     $TransDisp $TransDisp $LongDisp);

diffusiveGrowth diffusiveGrowth [0 2 -1 0 0 0 0]  1.0E-10;

Hacc        Hacc        [0 0 0 0 0 0 0]  3.2E-02; //1/21.58 = 4.63E-2 in [Berlin 2014]
O2_saturation        O2_saturation        [1 -3 0 0 0 0 0]  9.0; // Remember this is wrong
oxygen_mass_transfer	oxygen_mass_transfer	[0 0 -1 0 0 0 0] 5.787E-5;  //5 day^-1

