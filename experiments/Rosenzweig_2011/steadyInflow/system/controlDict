FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

end_days 22.83; // 22.83;
end_secs #calc "round($end_days*86400.0)";

write_days 0.25;
write_secs #calc "round($write_days*86400.0)";

application     unsatNutrientCycle;

startFrom       latestTime;
startTime       0;
stopAt          endTime;

endTime	        $end_secs;
writeControl    runTime;
writeInterval   $write_secs;

deltaT          0.1;
purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   3;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1.0;

debug           1;

