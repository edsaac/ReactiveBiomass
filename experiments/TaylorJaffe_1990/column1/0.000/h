FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 0 0 0 0 0 ];

internalField   uniform 0.001;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    right
    {
        type            empty;
    }
    left
    {
        type            empty;
    }
    top
    {
        type            fixedValue;
        value           uniform 0.001;
    }
    bottom
    {
        type            fixedGradient;
        gradient        uniform 0.04346;
    }
}


