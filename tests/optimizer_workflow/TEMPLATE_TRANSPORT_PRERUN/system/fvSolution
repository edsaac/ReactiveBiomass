FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          fvSolution;
}

solvers
{
    h
    {
        solver          PCG;
        preconditioner  FDIC;
        tolerance       1e-14;
        relTol          0;
    }
    "(DOC|POC|O2|NO3|NH4|XI|EPS|UAP|BAP|POCr|XARp|XNp|XDNp|tracer)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-09;
        relTol          0;
    }
    "(XAR|XN|XDN)"
    {
        solver          PBiCGStab;
        preconditioner  DIC;
        tolerance       1e-09;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
}

timeStepping
{
    convergeThreshold 0.09;
    maxItersToConverge 100;
    UpDeltaT_nIters 3;
    UpDeltaT_Factor 1.15;
    maxDeltaT       10;
    DownDeltaT_Factor 0.5;
    minDeltaT       1e-09;
    hguess_coeff    0.9;
}


