/*--------------------------------*- C++ -*----------------------------------*\
  =========                 |
  \\      /  F ield         | OpenFOAM: The Open Source CFD Toolbox
   \\    /   O peration     | Website:  https://openfoam.org
    \\  /    A nd           | Version:  7
     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}

application     unsatNutrientCycle;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         864000;

writeControl    runTime;

writeInterval   8640;

deltaT          0.0001;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      fixed;

timePrecision   3;

runTimeModifiable false;

adjustTimeStep  yes;

maxCo           1;

debug           1;

functions
{
    boundaryProbes
    {
        fields          ( "Sw" "h" "U" );
        points          ( ( 0.1 0.1 0 ) ( 0.1 0.1 2 ) );
        maxDistance     0.01;
        interpolationScheme cell;
        setFormat       raw;
        setConfig
        {
            type            lineUniform;
            axis            distance;
            nPoints         100;
        }
        patches         ( "bottom" "top" );
        type            sets;
        libs            ( "libsampling.so" );
        executeControl  timeStep;
        writeControl    runTime;
        sets            ( points { type boundaryPoints ; axis xyz ; points ( ( 0.1 0.1 0 ) ( 0.1 0.1 2 ) ) ; maxDistance 0.01 ; patches ( "bottom" "top" ) ; } );
        writeInterval   60;
    }
}


// ************************************************************************* //
