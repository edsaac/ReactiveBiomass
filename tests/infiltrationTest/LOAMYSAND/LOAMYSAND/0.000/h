FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform -0.462937;

boundaryField
{
    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    right
    {
  	type		empty;
    }

    left
    {
  	type		empty;
    }

    top
    {

        type    fixedValue;
        value   uniform 0.05;
    }

    bottom
    {
        // Set to 1.0 for a NO-FLOW condition
        // Set to 0.0 for a unit gradient drain
        type        fixedGradient;
        gradient    uniform 0;   

        // type    fixedValue;
        // value   uniform 0.0;

    }
}

