FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue K_s 0.1
);

regions
(
    boxToCell
    {
        box (0.4 0.0 0) (0.6 0.20 0.70);
        fieldValues
        (
            volScalarFieldValue K_s 100.
        );
    }
);


