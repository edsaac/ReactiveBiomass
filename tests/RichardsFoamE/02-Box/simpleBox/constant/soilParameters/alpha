FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.000";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 3.60;

boundaryField
{
    "(top|left|right|bottom)"
    {
        type            zeroGradient;
    }
    "(front|back)"
    {
        type            empty;
    }
}


