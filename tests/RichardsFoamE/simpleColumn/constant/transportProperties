// [kg m s K mol * *]

// Diameters
ds        ds        [0 1 0 0 0 0 0]     2.0E-3;
dp        dp        [0 1 0 0 0 0 0]     10.0E-6;

// Densities
rho       rho       [1 -3 0 0 0 0 0]    999.79;
rho_clay  rho_clay  [1 -3 0 0 0 0 0]    1050.0;
