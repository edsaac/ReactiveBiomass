FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    h
    {
        solver          PCG;
        preconditioner  FDIC;
        tolerance       1e-20;
        relTol          0;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 1;
}

relaxFactor 0.995;

relaxationFactors
{
    // fields  //Explicit relaxation
    // {

    // }
    
    equations  //Implicit relaxation
    {
        h        $relaxFactor;
    }
}

