FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 100;

boundaryField
{
    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    right
    {
  	type		empty;
    }

    left
    {
  	type		empty;
    }

    top
    {
        type        fixedValue;
	    value		uniform   101.1; // 1.004672127E+01;
    }

    bottom
    {
        //type            zeroGradient;
        type        fixedValue;
        value       uniform   100; // 1.004672127E+01;

    }
}

