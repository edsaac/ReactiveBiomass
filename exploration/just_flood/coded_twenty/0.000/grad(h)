FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.000";
    object      grad(h);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    left
    {
        type            empty;
    }
    right
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    bottom
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
}


